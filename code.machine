# "C": 0b0001, # "carry" bit
# "O": 0b0010, # "overflow" bit
# "Z": 0b0100, # "zero" bit
# "N": 0b1000, # "Negative sign" bit

# display string

jmp start

x: mem 16 = "ABCDEF"

start:

xfr r4 continue $0
xfr r5 continue $1

xfr r0 x

continue:

	xfr r1 [r0]

	eor r1 0 	# test character
	jze done

	dsp r1

	adc r0 1
	jmp r4, r5

done: 
